// по въведеното N ест число (петцифри) да се конструира първоначално M, получено от даденото в обратен ред на цифрите на N

#include <iostream>
using namespace std;
int main(){
    int N, M=0;
    cin >> N;
    while (N>0){
        M= M * 10 + (N % 10);
        N /= 10;       
    }
    cout << M;
    return 0;
}
